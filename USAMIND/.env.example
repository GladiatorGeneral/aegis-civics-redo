# Environment Configuration
# Copy this file to .env.local and fill in the values

# API Configuration
NEXT_PUBLIC_API_URL=https://api.usamind.ai
NEXT_PUBLIC_WS_URL=wss://stream.usamind.ai

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/usamind

# Redis Cache
REDIS_URL=redis://localhost:6379

# Congress API
CONGRESS_API_KEY=your_congress_api_key

# AI Services
AI_MODEL_ENDPOINT=https://ai.usamind.ai
TENSORFLOW_MODEL_PATH=/models/legislative-predictor

# Authentication
NEXTAUTH_SECRET=your-nextauth-secret
NEXTAUTH_URL=http://localhost:3000

# Blockchain
BLOCKCHAIN_NETWORK=mainnet
BLOCKCHAIN_RPC_URL=https://rpc.usamind.ai

# Monitoring
SENTRY_DSN=https://your-sentry-dsn
PROMETHEUS_ENDPOINT=http://localhost:9090

# Feature Flags
ENABLE_AR_MODE=false
ENABLE_QUANTUM_PREDICTION=true
ENABLE_BLOCKCHAIN_VERIFICATION=true

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100

# Edge Cache
EDGE_CACHE_TTL=5000
EDGE_CACHE_MAX_SIZE=10000
